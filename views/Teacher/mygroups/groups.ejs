		<form action='/mygroups/<%= grp.Group %>/addroll'>
			<button class="btn btn-primary" type='submit'>
				 <h3><span class="badge">Add New Roll numbers</span></h3>
			  </button>
		</form>
		
		<%
			for(var v=1; v <= grp.roll.length; v++) {
			%>
			<span class="badge badge-light"> <%= grp.roll[v] %></span><br>
		<% } %>
